<BorderPane prefHeight="810.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cinema.control.AllBookingsController">
   <top>
      <AnchorPane prefHeight="83.0" prefWidth="615.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar layoutY="46.0" prefHeight="36.0" prefWidth="760.0">
               <menus>
                  <Menu fx:id="home" mnemonicParsing="false" onAction="#goToHome" text="HOME">
                     <items>
                        <MenuItem mnemonicParsing="false" text="HOME" />
                     </items>
                  </Menu>
                  <Menu fx:id="whatsOn" mnemonicParsing="false" onAction="#goToWhatsOn" text="WHAT'S ON">
