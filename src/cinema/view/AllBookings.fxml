                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="futureExportButton" layoutX="437.0" layoutY="519.0" mnemonicParsing="false" onAction="#exportsFutureBookings" prefHeight="54.0" prefWidth="160.0" text="Export all future film booking details" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Label fx:id="futureBookingLabel" layoutX="440.0" layoutY="600.0" prefHeight="72.0" prefWidth="217.0" wrapText="true" />
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
