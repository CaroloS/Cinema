                  </Menu>
                  <Menu fx:id="allBookings" mnemonicParsing="false" text="ALL BOOKINGS">
                     <items>
                        <MenuItem mnemonicParsing="false" text="ALL BOOKINGS" />
                     </items>
                  </Menu>
                  <Menu disable="true" mnemonicParsing="false" text="                                                                                              ">
                     <items>
                        <MenuItem mnemonicParsing="false" text="                                                                                          " />
                     </items>
                  </Menu>
                  <Menu fx:id="logOut" mnemonicParsing="false" onAction="#logsOut" text="LOG OUT">
                     <items>
                        <MenuItem fx:id="logOut" mnemonicParsing="false" onAction="#logsOut" text="LOG OUT" />
                     </items>
                  </Menu>
               </menus>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </MenuBar>
         </children>
      </AnchorPane>
   </top>
   <center>
      <ScrollPane prefHeight="614.0" prefWidth="643.0" BorderPane.alignment="CENTER">
         <content>
            <AnchorPane prefHeight="724.0" prefWidth="758.0">
               <children>
                  <Label fx:id="pastBookingLabel" layoutX="131.0" layoutY="602.0" prefHeight="68.0" prefWidth="208.0" wrapText="true" />
                  <AnchorPane fx:id="pastPieAnchor" layoutY="64.0" prefHeight="407.0" prefWidth="417.0" />
                  <AnchorPane fx:id="futurePieAnchor" layoutX="303.0" layoutY="65.0" prefHeight="406.0" prefWidth="455.0" />
